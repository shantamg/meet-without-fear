{
  "expo": {
    "name": "Meet Without Fear",
    "slug": "meet-without-fear",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "meetwithoutfear",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.galuten.meetwithoutfear",
      "buildNumber": "14",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app needs access to your microphone to record audio sessions."
      },
      "appleTeamId": "446P429C8Q"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#111827"
      },
      "icon": "./assets/images/icon.png",
      "package": "com.meetwithoutfear.app",
      "versionCode": 13,
      "permissions": [
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-router",
        {
          "root": "./app"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#111827"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f381a744-a85d-478c-a8dd-75d54414aa5a"
      },
      "apiUrl": "https://meet-without-fear-api.onrender.com",
      "router": {
        "root": "./app"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "owner": "shantamg"
  }
}
